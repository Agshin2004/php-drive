{
  "require": {
    "slim/slim": "4.*",
    "slim/psr7": "^1.7",
    "illuminate/database": "^12.16",
    "symfony/dotenv": "^7.3",
    "firebase/php-jwt": "^6.11",
    "php-di/php-di": "^7.0",
    "psr/container": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.75",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "cs": "vendor/bin/php-cs-fixer check",
    "cs:fix": "vendor/bin/php-cs-fixer fix",
    "stan": "vendor/bin/phpstan analyse"
  }
}
